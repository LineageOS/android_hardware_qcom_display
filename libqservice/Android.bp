cc_library_shared {
    name: "libqservice",
    defaults: ["qtidisplay_defaults"],
    vendor: true,
    shared_libs: ["libbinder"],
    cflags: [
        "-DLOG_TAG=\"qdqservice\"",
        "-Wno-sign-conversion",
    ],
    srcs: [
        "QService.cpp",
        "IQService.cpp",
        "IQClient.cpp",
        "IQHDMIClient.cpp",
    ],
    sanitize: {
        misc_undefined: [
            "signed-integer-overflow",
            "unsigned-integer-overflow",
        ],
    },
}
